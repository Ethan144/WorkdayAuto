{
  "name": "WorkdayAuto",
  "version": "1.0",
  "description": "Automatically fill in workday apps",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "storage",
    "activeTab"
  ],

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "randoncat.png"
  },

  "background": {
		"scripts": ["popup.js"]
	},

  "manifest_version": 2

  
}

